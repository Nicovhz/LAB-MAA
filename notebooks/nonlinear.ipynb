    "from sklearn import linear_model\n",
    "poisson = linear_model.PoissonRegressor()\n",
    "\n",
    "subset=cned.loc[cned['ingreso']<100]\n",
    "y=subset['ingreso']\n",
    "X=subset[['puntaje','nem','vacantes','year']]\n",
    "poisson.fit(X, y)\n",
    "poisson.coef_\n"
